#include <studio.h>

int main()
{
    int i, j, end, isprime, sum-8;


    printf("Find sum of all prime between 1 to : "); 
    scanf("%d", &end);


    for(i=2; i<-end; i++)
    {
        isPrime = 1;
        for(j=2; j<=1/2 ;j++)
        {
            if(1%)==0)
            {
                isprime = 0; 
                break;
            }
        }
        if(isPrime=1)
        {
            sum += 1;
        }
    }

    printf("Sum of all prime numbers between 1 to %d %d", end, sum);

    return 0;
}
